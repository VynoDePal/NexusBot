# Next.js RAG Chatbot - Exemple d'environnement

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
SUPABASE_ANON_KEY=
# Optionnel: pour scripts/mutations côté serveur
SUPABASE_SERVICE_ROLE=

# OpenRouter (LLM)
OPENROUTER_API_KEY=
# Optionnels
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_HTTP_REFERRER=
OPENROUTER_X_TITLE=Next.js RAG Chatbot

# Embeddings (OpenAI)
OPENAI_API_KEY=
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# RAG
RAG_MATCH_THRESHOLD=0.78
RAG_MATCH_COUNT=8

# Embeddings provider (default: supabase via Transformers.js)
EMBEDDINGS_PROVIDER=supabase
# For Supabase provider (gte-small, 384 dims)
SUPABASE_EMBEDDING_MODEL=Supabase/gte-small
# To switch to OpenAI, set EMBEDDINGS_PROVIDER=openai and provide OPENAI_API_KEY
